{
    "id": "/AddCustomer",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Add New Customer",
    "type": "object",
    "additionalProperties": true,
    "required": [
        "ID",
        "UserName",
        "EmailID",
        "ContactNumber",
        "Password"
    ],
    "properties": {
        "ID": {
            "type": ["string"],
            "description": "Unique ID"
        },
        "UserName": {
            "type": [
                "string"
            ],
            "minLength": 2,
            "maxLength": 50,
            "description": "User Name"
        },
        "EmailID": {
            "type": [
                "string"
            ],
            "minLength": 2,
            "maxLength": 50,
            "description": "User Email id"
        },
        "ContactNumber": {
            "type": [
                "integer"
            ],
            "maxLength": 10,
            "description": "Contact Number"
        },
        "Password": {
            "type": [
                "string"
            ],
            "maxLength": 100,
            "description": "Password"
        },
        "LocationName": {
            "type": [
                "string"
            ],
            "minLength":2,
            "maxLength": 100,
            "description": "Location Name"
        },
        "Isactive": {
            "type": [
                "boolean"
            ],
            "maxLength": 1,
            "description": "Is Active"
        },
        "VehicleID": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Vehicle Id List"
        },
        "LoanID": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Loan Id List"
        },
        "WhishlistID": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Whislist Id List"
        },
        "PurchasedAccessoriesID": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Purchase Accessories Id List"
        },
        "LoanAgreementtemplate": {
            "type": "string",
            "description": "Loan Agreement Image"
        },
        "CreatedAt": {
            "type": "string",
            
            "description": "Created At Date"
        },
        "Type": {
            "type": "string",
            "description": "Type"
        }
    }
}