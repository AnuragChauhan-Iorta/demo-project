{
    "id": "/AddCustomer",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Add Vehicle",
    "type": "object",
    "additionalProperties": true,
    "required": [
        "id",
        "vehicle_service_type",
        " vehicle_type",
        "vehicle_number",
        "registration_place",
        "current_location",
        "no_of_owners"
    ],
    "properties": {
        "id": {
            "type": ["string"],
            "description": "Unique ID"
        },
        "vehicle_service_type": {
            "type":"string" 
        },
        " vehicle_type": {
            "type":"string"
        },
        "vehicle_number": {
            "type":"string"   
        },
        " make": {
            "type":"string"
        },
        "model": {
            "type":"string"
        },
        "varient": {
           "type":"string"
        },
        "year": {
          "type":"string",
          "format": "date"
        },
        "color":{
            "type":"string"
        },
        "registration_place":{
            "type":"string"
        },
        "current_location":{
            "type":"string"
        },
        "fuel_type":{
            "type":"string"
        },
        "transmission_type":{
            "type":"string"
        },
        "gares":{
            "type":"string"
        },
        "price":{
            "type":"string"
        },
        "vehicle_category":{
            "type":"string"
        },
        "no_of_owners":{
            "type":"number"
        },
        "engine_capcity_cc":{
            "type":"string"
        },
        "vehicle_full_details": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Vehicle  Details"
        },
        "vehicle_image_id": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Vehicle Image Id"
        },
        "createdat": {
            "type": "string",
            "format": "date",
            "description": "Created At Date"
        },
        "createdby": {
            "type": "string"
        }
    }
}